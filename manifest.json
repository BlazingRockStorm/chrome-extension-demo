{
  "name": "Red Senshi Nemesis",
  "description": "The extension that show the nemesis of the Red Senshi in Super Sentai",
  "version": "0.0.1",
  "manifest_version": 2,
  "browser_action": {
    "default_title": "Behold! The best opponent of the Red!",
    "default_icon": "icons/icon16.png"
  },
  "icons": { 
    "16": "icons/icon16.png",
    "32": "icons/icon32.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png" 
  },
  "permissions": [
    "activeTab"
  ],
  "web_accessible_resources": ["*.jpg","*.JPG","*.jpeg","*.JPEG"],
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  }
}
